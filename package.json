{
  "name": "@kintone/plugin-uploader",
  "version": "2.4.8",
  "description": "A kintone plugin uploader using puppetieer",
  "bin": {
    "kintone-plugin-uploader": "bin/cli.js"
  },
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "release": "run-s build lint && standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/plugin-uploader",
  "homepage": "https://github.com/kintone/plugin-uploader",
  "bugs": "https://github.com/kintone/plugin-uploader/issues",
  "dependencies": {
    "chalk": "^2.4.2",
    "inquirer": "^6.2.2",
    "meow": "^5.0.0",
    "os-locale": "^3.1.0",
    "puppeteer": "^1.14.0"
  },
  "devDependencies": {
    "@types/inquirer": "6.0.3",
    "@types/node": "^9.6.49",
    "@types/puppeteer": "^1.12.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "standard-version": "^5.0.2",
    "tslint": "^5.15.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  }
}
