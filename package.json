{
  "name": "@kintone/plugin-uploader",
  "version": "4.1.1",
  "description": "A kintone plugin uploader using puppetieer",
  "bin": {
    "kintone-plugin-uploader": "bin/cli.js"
  },
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "tsc:check": "tsc --noEmit",
    "lint": "eslint --ext .ts,.js src bin",
    "release": "run-s build lint && standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/plugin-uploader",
  "homepage": "https://github.com/kintone/plugin-uploader",
  "bugs": "https://github.com/kintone/plugin-uploader/issues",
  "dependencies": {
    "chalk": "^4.1.0",
    "inquirer": "^7.3.3",
    "meow": "^7.0.1",
    "os-locale": "^5.0.0",
    "puppeteer": "4.0.1"
  },
  "devDependencies": {
    "@cybozu/eslint-config": "^10.0.4",
    "@types/inquirer": "7.3.0",
    "@types/node": "^9.6.57",
    "@types/puppeteer": "^3.0.1",
    "eslint": "^6.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.2",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.9.7"
  }
}
