{
  "name": "@kintone/plugin-uploader",
  "version": "4.0.0",
  "description": "A kintone plugin uploader using puppetieer",
  "bin": {
    "kintone-plugin-uploader": "bin/cli.js"
  },
  "main": "dist/src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "tsc:check": "tsc --noEmit",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "release": "run-s build lint && standard-version",
    "release:dryrun": "standard-version --dry-run"
  },
  "files": [
    "bin",
    "dist"
  ],
  "keywords": [
    "kintone"
  ],
  "author": "kintone",
  "license": "MIT",
  "repository": "kintone/plugin-uploader",
  "homepage": "https://github.com/kintone/plugin-uploader",
  "bugs": "https://github.com/kintone/plugin-uploader/issues",
  "dependencies": {
    "chalk": "^3.0.0",
    "inquirer": "^7.0.4",
    "meow": "^6.0.0",
    "os-locale": "^4.0.0",
    "puppeteer": "^2.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "6.5.0",
    "@types/node": "^9.6.55",
    "@types/puppeteer": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "tslint": "^5.20.1",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.5"
  }
}
